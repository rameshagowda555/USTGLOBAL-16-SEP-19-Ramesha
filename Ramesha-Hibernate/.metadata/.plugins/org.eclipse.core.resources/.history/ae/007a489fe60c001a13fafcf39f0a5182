package com.ustglobal.jpawithhibernateapp.onetoonemapping;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;


import lombok.Data;

@Data
@Entity
@Table(name="person")
public class Person {
	@Id
	@Column
	private int pid;
	@Column
	private String name;
	
}

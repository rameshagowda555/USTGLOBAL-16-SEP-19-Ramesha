package com.ustglobal.java8features;

import java.util.function.Predicate;

public class TestB {
	public static void main(String[] args) {
		
		Predicate<Student> p = (s)->
		{
			if(s.percentage>35) {
				return true;
			}else {
				return false;
			}
		};
		p.test(s);
		
		
		
		
		Student s = new Student(101, "Ramesha", 83.02);
	}
}
